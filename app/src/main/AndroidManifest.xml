<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="boulet.com.outerspacemanager.outerspacemanager">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateAccountActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".GalaxyActivity" />
        <activity android:name=".BuildingActivity" />
        <activity android:name=".BuildingDetailsActivity" />
        <activity android:name=".GeneralActivity" />
        <activity android:name=".FlotteActivity" />
        <activity android:name=".VaisseauActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".SearchDetailsActivity" />
        <activity android:name=".ChantierActivity" />
    </application>

</manifest>